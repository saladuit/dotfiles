#This is a custom ZSH configuration file, Put anything you would like to change in here since the other config files get overwritten when you run the script again to update.

#42 Header Information
export INTRAUSER=$USER					#Change this to your Intra Username
export MAIL="safoh@student.codam.nl"	#Change this to your 42 Email Adress (username@student.codam.nl for Codam Students)

#Custom Aliases
alias please='sudo'														#Please just sounds nicer than sudo, Time to be nice to your computer.
alias phone='ssh houtworm@phone'										#Pretty personal, But handy if you have any SSH capable devices around the house.
alias server='ssh houtworm@server'										#Pretty personal, But handy if you have any SSH capable devices around the house.
alias game='gamemoderun mangohud' 										#This is used by me to start games with these 2 tools.
alias ytflac="youtube-dl -x --audio-format flac --prefer-ffmpeg" 		#Downloads youtube video and converts it to FLAC. (Audio Only)
alias ytmkv="youtube-dl -F" 											#Downloads youtube video in the original format
alias lol='lolcat' 														#Pipe anything into lol to make it like a rainbow, Try "nvim | lol" for example, Requires lolcat to be installed.
alias clearhome='echo -n "Available before:\t"; df -h | grep /Users/lde-la-h | sed '\''s/  */:/g'\'' | cut -d '\'':'\'' -f 4; unsetopt nomatch; rm -Rf ~/Library/*.42_cache_bak*; rm -Rf ~/*.42_cache_bak_*; setopt nomatch; echo -n "Available after:\t"; df -h | grep /Users/lde-la-h | sed '\''s/  */:/g'\'' | cut -d '\'':'\'' -f 4;'
